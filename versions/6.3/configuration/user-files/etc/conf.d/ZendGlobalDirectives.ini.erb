; The installation directory
zend.install_dir=<%= ENV['OPENSHIFT_ZEND_DIR'] %>php-5.5

; The directory all configuration files are stored in
zend.conf_dir=<%= ENV['OPENSHIFT_ZEND_DIR'] %>php-5.5/etc

; The directory all *.pid files are stored in
zend.pid_dir=<%= ENV['OPENSHIFT_ZEND_DIR'] %>php-5.5/var/run

; The directory all temporary files are stored in
zend.temp_dir=<%= ENV['OPENSHIFT_ZEND_DIR'] %>php-5.5/tmp

; The directory all log files are stored in
zend.log_dir=<%= ENV['OPENSHIFT_ZEND_DIR'] %>php-5.5/var/log

; The directory all log files are stored in
zend.data_dir=<%= ENV['OPENSHIFT_ZEND_DIR'] %>php-5.5/var

; UID of httpd process
zend.httpd_uid=48

; GID of httpd process
zend.httpd_gid=48

; The directory where PHP configuration files are located
zend.ini_scandir=conf.d

; The serial number
zend.serial_number=RLT27J40C41G21A9671206B9E5EAB8F1

; The user name
zend.user_name=zend-trial

; Zend Server Node ID
zend.node_id=0

; The IP address on which the user's webserver is listening
zend.user_webserver_ip=127.0.0.1

; The port number on which the user's webserver is listening
zend.user_webserver_port=10083

; The webserver type
zend.webserver_type=apache

; When set, each event generated by the Monitor is generated as a unique event (i.e. it will be shown as a new event)
zend.monitor_generate_unique_events=0

; When set, all monitor events are immediately reported to the database (affects performance)
zend.monitor_disable_event_buffering=0

; The port on which Deployment Daemon accepts requests (the IP is always on loopback, i.e. 127.0.0.1)
zend.deployment_daemon_port=10064


zend.php_sapi_version_header="X-Powered-By: PHP/5.5.7 ZendServer/6.3.0"
